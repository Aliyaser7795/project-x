@model string

<div style="width:100%">
    @if (Model == SD.StatusSubmitted)
    {

        <img src="~/images/Submitted.PNG" />

    }

    @if (Model == SD.StatusInProcess)
    {

       <img src="~/images/Process.PNG" />

    }

    @if (Model == SD.StatusResdy)
    {
          <img src="~/images/Ready.PNG" />

    }

    @if (Model == SD.StatusCompleted)
    {
         <img src="~/images/Completed.PNG" />

    }

</div>